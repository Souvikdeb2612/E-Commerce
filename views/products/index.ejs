<%- include("../partials/header") %> 
<%- include("../partials/navbar2") %> 

<div class="container">
    <div class="row">
        <% for (let product of products){%> 
            <div class="col-lg-4">
           
            <div class="card" style="width: 20rem;">
                <img src="<%=product.img%>" class="card-img-top" alt="...">
                <div class="card-body">
                  <h5 class="card-title"><%=product.name%></h5>
                  <p class="card-text text-center font-weight-bold">&#8377;<%=product.price%></p>
                  <a href="/products/<%= product._id %>/show " class="btn btn-card ">KNOW MORE</a>
                </div>
              </div>
            </div>
        <%}%>
        
    </div>
      
</div>
<%- include("../partials/footer") %>  